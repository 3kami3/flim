(setq flim-modules '(std11
                     mime-def
		     mel mel-ccl mel-b mel-q mel-u mel-g
                     closure
                     natset
                     digraph
                     automata rx lex
                     lr-driver
                     ew-var
                     ew-util
                     ew-line
                     ew-quote
                     ew-bq
                     ew-unit
                     ew-data
                     ew-parse
                     ew-scan-s
                     ew-scan-m
                     ew-scan-u
                     ew-scan-n
                     ew-dec
		     eword-decode eword-encode
		     mime mime-parse mmbuffer mmcooked
		     mailcap
                     ))

(if (fboundp 'dynamic-link)
    (setq flim-modules (cons 'mel-dl flim-modules))
  )


;;; -*-Emacs-Lisp-*-

;; FLIM-ELS: list of FLIM modules to install

;;; Code:

(setq flim-modules '(std11
                     mime-def
		     mel mel-b mel-q mel-u mel-g
                     closure
                     natset
                     digraph
                     automata rx lex
                     lr-driver
                     ew-var
                     ew-util
                     ew-line
                     ew-quote
                     ew-bq
                     ew-unit
                     ew-data
                     ew-parse
                     ew-scan-s
                     ew-scan-m
                     ew-scan-u
                     ew-scan-n
                     ew-dec
		     eword-decode eword-encode
                     ew-compat
		     mime mime-parse mmbuffer mmcooked
		     mailcap
                     ))

(if (fboundp 'dynamic-link)
    (setq flim-modules (cons 'mel-b-dl flim-modules))
  )

(if (and (featurep 'mule)
	 (not (and (boundp 'MULE) MULE)))
    (setq flim-modules (cons 'mel-ccl flim-modules))
  )

;;; FLIM-ELS ends here
